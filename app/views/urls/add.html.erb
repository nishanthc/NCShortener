<div class="subjects new">
  <h2>Create URL</h2>

  <%= form_for(:url, :url => {:action => 'create'}) do |f| %>

    <table summary="URL form fields">
      <tr>
        <th>Long_url</th>
        <td><%= f.text_field(:long_url) %></td>
      </tr>

    </table>

    <div class="form-buttons">
      <%= submit_tag("Create URL") %>
    </div>

  <% end %>
</div>
